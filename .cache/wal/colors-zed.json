{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0a0a0790",
                "editor.background": "#0a0a0790",
                "editor.foreground": "#c1c1c1",
                "text": "#c1c1c1",
                "text.muted": "#c1c1c170",
                "text.ignored": "#c1c1c140",
                "text.placeholder": "#c1c1c150",
                "ignored": "#c1c1c130",
                "element.hover": "#c1c1c130",
                "ghost_element.hover": "#2d7b8730",
                "ghost_element.selected": "#2d7b8730",
                "ghost_element.active": "#2d7b8760",
                "border": "#0a0a07",
                "editor.highlighted_line.background": "#2d7b8710",
                "editor.active_line.background": "#2d7b8710",
                "panel.background": "#0a0a0790",
                "title_bar.background": "#0a0a0790",
                "title_bar.inactive_background": "#0a0a0790",
                "status_bar.background": "#0a0a0790",
                "drop_target.background": "#0a0a0790",
                "elevated_surface.background": "#0a0a07",
                "toolbar.background": "#0a0a0790",
                "tab_bar.background": "#0a0a0790",
                "tab.inactive_background": "#0a0a0790",
                "tab.active_background": "#2d7b8730",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0a0a07",
                "scrollbar.thumb.background": "#2d7b87",
                "editor.gutter.background": "#0a0a0790",
                "terminal.background": "#0a0a0710",
                "terminal.foreground": "#c1c1c1",
                "terminal.dim_foreground": "#c1c1c1",
                "terminal.bright_foreground": "#c1c1c1",
                "terminal.ansi.black": "#0a0a07",
                "terminal.ansi.red": "#2d7b87",
                "terminal.ansi.green": "#388bab",
                "terminal.ansi.yellow": "#935f31",
                "terminal.ansi.blue": "#c4974f",
                "terminal.ansi.magenta": "#c99a5d",
                "terminal.ansi.cyan": "#d1ac74",
                "terminal.ansi.white": "#c1c1c1",
                "terminal.ansi.bright_black": "#676754",
                "terminal.ansi.bright_red": "#2d7b87",
                "terminal.ansi.bright_green": "#388bab",
                "terminal.ansi.bright_yellow": "#935f31",
                "terminal.ansi.bright_blue": "#c4974f",
                "terminal.ansi.bright_magenta": "#c99a5d",
                "terminal.ansi.bright_cyan": "#d1ac74",
                "terminal.ansi.bright_white": "#c1c1c1",
                "modified": "#2d7b87",
                "syntax": {
                    "attribute": {
                        "color": "#2d7b87"
                    },
                    "boolean": {
                      "color": "#388bab"
                    },
                    "comment": {
                      "color": "#676754"
                    },
                    "comment.doc": {
                      "color": "#676754"
                    },
                    "constant": {
                      "color": "#388bab"
                    },
                    "function": {
                      "color": "#d1ac74"
                    },
                    "keyword": {
                      "color": "#c4974f"
                    },
                    "number": {
                      "color": "#c99a5d"
                    },
                    "operator": {
                      "color": "#c4974f"
                    },
                    "string": {
                      "color": "#2d7b87"
                    },
                    "variable": {
                      "color": "#388bab"
                    }
                },
                "players": [
                  {
                    "cursor": "#2d7b87",
                    "background": "#0a0a07",
                    "selection": "#2d7b8730"
                  }
                ]
            }
        }
    ]
}
